{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable -A -",
    "manifest": "deno task cli manifest $(pwd)",
    "start": "deno run --allow-env --allow-read --allow-write --allow-run --allow-sys --allow-net --unstable-kv --watch=static/,routes/ dev.ts",
    "build": "deno run --allow-env --allow-read --allow-write --allow-net --unstable-kv dev.ts build",
    "preview": "deno run --allow-env --allow-read --allow-write --allow-net --unstable-kv main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "test": "deno test --allow-env --allow-read --allow-net"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "@kinde-oss/kinde-typescript-sdk": "npm:@kinde-oss/kinde-typescript-sdk@^2.9.1",
    "daisyui": "npm:daisyui@^4.12.14",
    "lucide-react": "https://esm.sh/lucide-react@0.468.0",
    "preact": "https://esm.sh/preact@10.22.0",
    "preact/": "https://esm.sh/preact@10.22.0/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "std/": "https://deno.land/std@0.216.0/",
    "std/assert": "https://deno.land/std@0.216.0/assert/mod.ts",
    "tailwindcss": "npm:tailwindcss@3.4.1",
    "tailwindcss/": "npm:/tailwindcss@3.4.1/",
    "tailwindcss/plugin": "npm:/tailwindcss@3.4.1/plugin.js"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "nodeModulesDir": "auto"
}
